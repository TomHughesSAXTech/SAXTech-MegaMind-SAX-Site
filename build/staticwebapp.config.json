{
  "routes": [
    {
      "route": "/admin.html",
      "headers": {
        "Cache-Control": "no-store, must-revalidate",
        "Pragma": "no-cache",
        "Expires": "0"
      }
    },
    {
      "route": "/api/index/maintenance",
      "rewrite": "https://saxtechmegamindfunctions.azurewebsites.net/api/index/maintenance",
      "allowedRoles": ["anonymous"]
    }
  ],
  "navigationFallback": {
    "rewrite": "/index.html",
    "exclude": [
      "/api/*",
      "*.css",
      "*.scss",
      "*.js",
      "*.png",
      "*.gif",
      "*.ico",
      "*.jpg",
      "*.svg",
      "*.woff",
      "*.woff2",
      "*.ttf",
      "*.eot",
      "*.json",
      "*.txt"
    ]
  }
}
