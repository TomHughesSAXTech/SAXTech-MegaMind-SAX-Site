  "scoringProfiles": [
    {
      "name": "tax-relevance",
      "text": {
        "weights": {
          "section_title": 3.0,
          "section_id": 2.5,
          "keywords": 2.0,
          "summary": 1.5,
          "content": 1.0
        }
      },
      "functions": [
        {
          "type": "freshness",
          "fieldName": "indexed_date",
          "interpolation": "quadratic",
          "boost": 1.2,
          "freshness": {
            "boostingDuration": "P365D"
          }
        }
      ]
    }
  ],
  "defaultScoringProfile": "tax-relevance",
  "semantic": {
    "configurations": [
      {
        "name": "tax-semantic-config",
        "prioritizedFields": {
          "titleField": {
            "fieldName": "section_title"
          },
          "prioritizedContentFields": [
            {
              "fieldName": "content"
            },
            {
              "fieldName": "summary"
            }
          ],
          "prioritizedKeywordsFields": [
            {
              "fieldName": "keywords"
            }
          ]
        }
      }
    ],
    "defaultConfiguration": "tax-semantic-config"
  },
  "vectorSearch": {
    "algorithms": [
      {
        "name": "hnsw-algorithm",
        "kind": "hnsw",
        "hnswParameters": {
          "m": 16,
          "efConstruction": 400,
          "efSearch": 100,
          "metric": "cosine"
        }
      }
    ],
    "profiles": [
      {
        "name": "vector-profile",
        "algorithm": "hnsw-algorithm",
        "vectorizer": "openai-vectorizer"
