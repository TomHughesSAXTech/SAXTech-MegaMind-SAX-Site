{
  "routes": [
    {
      "route": "/api/conversations/*",
      "rewrite": "https://saxtechconversationlogs.azurewebsites.net/api/SaveConversationLog",
      "allowedRoles": ["anonymous"]
    },
    {
      "route": "/api/documents-search",
      "rewrite": "https://saxtechmegamindfunctions.azurewebsites.net/api/documents-search",
      "allowedRoles": ["anonymous"]
    },
    {
      "route": "/api/index/maintenance",
      "rewrite": "https://saxtechmegamindfunctions.azurewebsites.net/api/index/maintenance",
      "allowedRoles": ["anonymous"]
    }
  ],
  "navigationFallback": {
    "rewrite": "/index.html",
    "exclude": [
      "/api/*",
      "*.css",
      "*.scss",
      "*.js",
      "*.png",
      "*.gif",
      "*.ico",
      "*.jpg",
      "*.svg",
      "*.woff",
      "*.woff2",
      "*.ttf",
      "*.eot",
      "*.json",
      "*.txt"
    ]
  }
}